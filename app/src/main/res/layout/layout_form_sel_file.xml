<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:z="http://schemas.android.com/apk/res-auto"
    android:id="@+id/formSelFile"
    android:layout_width="280dp" android:layout_height="300dp"
    z:styles="@style/form" android:text="@string/formSelFile" z:styleCaption="@style/formCaption"
    z:gravity="center" z:formMode="none" z:modal="true" z:typeView="zFormSelFile" z:nameView="selfile">
    <LinearLayout
        android:layout_width="match_parent" android:layout_height="match_parent"
        z:pad="3dp" android:orientation="vertical"
        tools:ignore="UselessParent">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            z:styles="@style/diskHeadText" android:text="@string/choice_select_file"/>
        <ListView
            android:id="@+id/selList"
            z:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            z:styles="@style/setRibbon"/>
    </LinearLayout>
</RelativeLayout>
